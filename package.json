{
  "name": "storage-abstraction",
  "version": "1.1.3",
  "description": "Provides an API for storing and retrieving files from a storage; this storage can be local or in the cloud.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "tests/data/*",
    "tests/test.ts",
    "tests/test.jasmine.ts"
  ],
  "dependencies": {
    "@google-cloud/storage": "^2.5.0",
    "@types/ramda": "^0.26.12",
    "await-to-js": "^2.1.1",
    "aws-sdk": "^2.479.0",
    "dotenv": "^8.0.0",
    "glob": "^7.1.4",
    "ramda": "^0.26.1",
    "slugify": "^1.3.4",
    "uniquid": "^1.1.4"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.13",
    "@types/node": "^11.13.15",
    "@types/rimraf": "^2.0.2",
    "jasmine": "^3.4.0",
    "jasmine-ts": "^0.3.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "tsc-watch": "^2.2.1",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "test-await": "ts-node ./tests/test-await.ts",
    "test-google": "TYPE='TYPE_GOOGLE_CLOUD' jasmine-ts ./tests/*.ts",
    "test-amazon": "TYPE='TYPE_AMAZON_S3' jasmine-ts ./tests/*.ts",
    "test-local": "TYPE='TYPE_LOCAL' jasmine-ts ./tests/*.ts",
    "test": "npm run test-local",
    "test2": "ts-node ./tests/test",
    "test-all": "npm run test-local && npm run test-google && npm run test-amazon",
    "tsc2": "node_modules/.bin/tsc --module commonjs --esModuleInterop --declaration --outDir dist index.ts",
    "copy": "cd ./example/backend && ./copy.sh",
    "tsc": "node_modules/.bin/tsc",
    "tsc:cp": "npm run tsc && npm run copy",
    "tsc:watch": "tsc-watch --onSuccess 'npm run copy'",
    "prepublishOnly": "npm run tsc"
  },
  "keywords": [
    "storage",
    "google cloud",
    "amazon s3"
  ],
  "author": "daniel@tweedegolf.nl",
  "license": "MIT"
}